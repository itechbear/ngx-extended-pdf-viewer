{
  "name": "@itechbear/ngx-extended-pdf-viewer",
  "description": "Embedding PDF files in your Angular application. Highly configurable viewer including the toolbar, sidebar, and all the features you're used to.",
  "version": "13.0.0-alpha.4",
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/itechbear/ngx-extended-pdf-viewer.git"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/collection.json schematics/*/schema.json schematics/*/files/**/**/* ../../dist/ngx-extended-pdf-viewer/"
  },
  "author": {
    "name": "Stephan Rauh",
    "url": "https://www.beyondjava.net"
  },
  "homepage": "https://pdfviewer.net",
  "bugs": {
    "url": "https://github.com/stephanrauh/ngx-extended-pdf-viewer/issues"
  },
  "keywords": [
    "PDF",
    "pdfjs",
    "PDF viewer",
    "Angular",
    "ng2",
    "Angular 9 - 13",
    "customizable PDF viewer"
  ],
  "peerDependencies": {
    "@angular/common": "^9.0.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0",
    "@angular/core": "^9.0.0 || ^10.0.0 || ^11.0.0 || ^12.0.0 || ^13.0.0"
  },
  "dependencies": {
    "lodash.deburr": "^4.1.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  }
}
